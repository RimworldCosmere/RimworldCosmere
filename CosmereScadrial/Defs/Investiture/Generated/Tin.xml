        <description>Enhances senses like sight, hearing, and smell.</description>
        <modExtensions>
            <li Class="CosmereScadrial.DefModExtensions.MetalLinked">
                <metals>
                    <li>tin</li>
                </metals>
            </li>
        </modExtensions>
        <degreeDatas>
            <li>
                <label>Misting: Tineye</label>
                <description>Enhances senses like sight, hearing, and smell.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
