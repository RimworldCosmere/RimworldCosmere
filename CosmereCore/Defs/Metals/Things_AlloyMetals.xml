<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Pewter (Alloy) -->
	<ThingDef ParentName="BaseMetalBase">
		<defName>Pewter</defName>
		<label>pewter</label>
		<description>Pewter is a refined alloy formed by blending Tin with Copper. Known for its soft, workable nature and a muted, classic luster, pewter has been a favored material for decorative and utilitarian objects since ancient times.</description>
		<graphicData>
			<texPath>Item/Resource/Pewter</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.0</MarketValue>
			<Mass>0.9</Mass>
			<StuffPower_Armor_Sharp>0.9</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.45</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.35</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>0</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.85</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.95</BluntDamageMultiplier>
		</statBases>
		<deepCommonality>2</deepCommonality>
		<deepCountPerPortion>50</deepCountPerPortion>
		<deepLumpSizeRange>5~10</deepLumpSizeRange>
		<stuffProps>
			<commonality>0.6</commonality>
			<color>(150,150,150)</color>
			<statFactors>
				<MaxHitPoints>0.6</MaxHitPoints>
				<Beauty>3</Beauty>
				<Flammability>0.3</Flammability>
				<MeleeWeapon_CooldownMultiplier>1.2</MeleeWeapon_CooldownMultiplier>
			</statFactors>
		</stuffProps>
	</ThingDef>

