<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="RockBase">
        <defName>MineableIron</defName>
        <label>Iron Ore</label>
        <description>Unrefined Iron ore mined directly from the earth.</description>
        <graphicData>
            <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <color>(121, 113, 111)</color>
            <colorTwo>(94, 77, 76)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1500</MaxHitPoints>
        </statBases>
        <deepCommonality>0.8</deepCommonality>
        <deepCountPerPortion>40</deepCountPerPortion>
        <deepLumpSizeRange>35~65</deepLumpSizeRange>
        <building>
            <isResourceRock>true</isResourceRock>
            <veinMineable>true</veinMineable>
            <mineableThing>Iron</mineableThing>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.8</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>35~65</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereResources.DefModExtension.MetalsLinked">
                <metal>Iron</metal>
            </li>
        </modExtensions>
    </ThingDef>
</Defs>