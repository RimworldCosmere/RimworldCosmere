<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Tin -->
    <GeneDef ParentName="AbstractInvestitureGene">
        <defName>Cosmere_Misting_Tin</defName>
        <label>Tin Misting</label>
        <labelShortAdj>Tineye</labelShortAdj>
        <description>Enhances senses like sight, hearing, and smell.</description>
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>4</displayOrderInCategory>
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Tin</def>
            </li>
        </forcedTraits>
        <modExtensions>
            <li Class="CosmereScadrial.DefModExtensions.MetalLinked">
                <metals>
                    <li>tin</li>
                </metals>
            </li>
        </modExtensions>
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Tin</iconPath>
    </GeneDef>
    <TraitDef ParentName="AbstractInvestitureTrait">
        <defName>Cosmere_Misting_Tin</defName>
