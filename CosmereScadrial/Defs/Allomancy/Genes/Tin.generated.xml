<Defs>
    <!-- Misting: Tin -->
    <GeneDef ParentName="AbstractAllomancyInvestitureGene">
        <defName>Cosmere_Scadrial_Gene_MistingTin</defName>

        <label>tin misting</label>
        <labelShortAdj>tineye</labelShortAdj>

        <description>Enhances {PAWN}&#x27;s senses like sight, hearing, and smell.</description>

        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>4</displayOrderInCategory>

        <resourceLabel>tin</resourceLabel>
        <resourceDescription>Group: physical\nAxis: internal\nPolarity: pulling\n\nEnhances {PAWN}&#x27;s senses like sight, hearing, and smell.</resourceDescription>

        <forcedTraits>
            <li>
                <def>Cosmere_Scadrial_Trait_MistingTin</def>
            </li>
        </forcedTraits>

        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Tin</metal>
            </li>
        </modExtensions>

            <abilities>
                    <li>Cosmere_Scadrial_Ability_Tin</li>
