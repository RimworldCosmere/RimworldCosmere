<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Pewter -->
    <GeneDef ParentName="AbstractInvestitureGene">
        <defName>Cosmere_Misting_Pewter</defName>
        <label>pewter misting</label>
        <labelShortAdj>thug</labelShortAdj>
        <description>Enhances physical strength and durability, useful in combat.</description>
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>5</displayOrderInCategory>
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Pewter</def>
            </li>
        </forcedTraits>
        <modExtensions>
            <li Class="CosmereMetals.ModExtensions.MetalsLinked">
                <metal>Pewter</metal>
            </li>
        </modExtensions>
        <abilities>
            <li>Cosmere_Ability_Pewter</li>
        </abilities>
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Pewter</iconPath>
    </GeneDef>
    <TraitDef ParentName="AbstractInvestitureTrait">
        <defName>Cosmere_Misting_Pewter</defName>
        <label>thug</label>
        <description>Enhances physical strength and durability, useful in combat.</description>
        <modExtensions>
            <li Class="CosmereMetals.ModExtensions.MetalsLinked">
                <metal>Pewter</metal>
            </li>
        </modExtensions>
        <degreeDatas>
            <li>
                <label>misting: thug</label>
                <description>Enhances physical strength and durability, useful in combat.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
