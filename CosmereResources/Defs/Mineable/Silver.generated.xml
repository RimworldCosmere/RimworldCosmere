            <mineableScatterCommonality>0.3</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>4~12</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereResources.DefModExtension.MetalsLinked">
                <metal>Silver</metal>
            </li>
        </modExtensions>
    </ThingDef>
