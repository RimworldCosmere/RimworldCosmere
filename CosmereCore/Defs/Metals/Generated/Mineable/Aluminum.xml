<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="AbstractMineable">
        <defName>MineableAluminum</defName>
        <label>Aluminum Ore</label>
        <description>Unrefined Aluminum ore mined directly from the earth.</description>
        <graphicData>
            <color>(163, 163, 163)</color>
            <colorTwo>(194, 194, 194)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1700</MaxHitPoints>
        </statBases>
        <building>
            <mineableThing>Aluminum</mineableThing>
            <mineableYield>25</mineableYield>
            <mineableScatterCommonality>0.2</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>20~30</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereCore.ModExtensions.MetalLinked">
                <metal>Aluminum</metal>
            </li>
        </modExtensions>
    </ThingDef>
</Defs>