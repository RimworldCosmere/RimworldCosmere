<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Aluminum -->
    <GeneDef ParentName="AbstractAllomancyInvestitureGene">
        <defName>Cosmere_Misting_Aluminum</defName>
        
        <label>aluminum misting</label>
        <labelShortAdj>gnat (aluminum)</labelShortAdj>
        
        <description>Shields {PAWN} from investiture. Also wipes away all other investiture reserves.</description>
        
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>10</displayOrderInCategory>
        
        <resourceLabel>aluminum</resourceLabel>
        <resourceDescription>Group: enhancement\nAxis: internal\nPolarity: pulling\n\nShields {PAWN} from investiture. Also wipes away all other investiture reserves.</resourceDescription>
        
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Aluminum</def>
            </li>
        </forcedTraits>
        
        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Aluminum</metal>
            </li>
        </modExtensions>
        
        <abilities>
            <li>Cosmere_Scadrial_Ability_Aluminum</li>
        </abilities>
        
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Aluminum</iconPath>
    </GeneDef>
    
    <TraitDef ParentName="AbstractMetallicArtsInvestitureTrait">
