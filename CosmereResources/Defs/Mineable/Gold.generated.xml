<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="RockBase">
        <defName>MineableGold</defName>
        <label>Gold Ore</label>
        <description>Unrefined Gold ore mined directly from the earth.</description>
        <graphicData>
            <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <color>(206, 172, 55)</color>
            <colorTwo>(136, 101, 23)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1500</MaxHitPoints>
        </statBases>
        <deepCommonality>0.3</deepCommonality>
        <deepCountPerPortion>40</deepCountPerPortion>
        <deepLumpSizeRange>2~8</deepLumpSizeRange>
        <building>
            <isResourceRock>true</isResourceRock>
            <veinMineable>true</veinMineable>
            <mineableThing>Gold</mineableThing>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.3</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>2~8</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereResources.DefModExtension.MetalsLinked">
                <metal>Gold</metal>
            </li>
        </modExtensions>
    </ThingDef>
</Defs>