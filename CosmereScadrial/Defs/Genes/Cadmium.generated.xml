<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Cadmium -->
    <GeneDef ParentName="AbstractInvestitureGene">
        <defName>Cosmere_Misting_Cadmium</defName>
        <label>cadmium misting</label>
        <labelShortAdj>pulser</labelShortAdj>
        <description>Slows down time in a bubble, creating a temporal distortion.</description>
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>14</displayOrderInCategory>
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Cadmium</def>
            </li>
        </forcedTraits>
        <modExtensions>
            <li Class="CosmereMetals.ModExtensions.MetalsLinked">
                <metal>Cadmium</metal>
            </li>
        </modExtensions>
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Cadmium</iconPath>
    </GeneDef>
    <TraitDef ParentName="AbstractInvestitureTrait">
        <defName>Cosmere_Misting_Cadmium</defName>
        <label>pulser</label>
        <description>Slows down time in a bubble, creating a temporal distortion.</description>
        <modExtensions>
            <li Class="CosmereMetals.ModExtensions.MetalsLinked">
                <metal>Cadmium</metal>
            </li>
        </modExtensions>
        <degreeDatas>
            <li>
                <label>misting: pulser</label>
                <description>Slows down time in a bubble, creating a temporal distortion.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
