<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="RockBase">
        <defName>MineableSilver</defName>
        <label>Silver Ore</label>
        <description>Unrefined Silver ore mined directly from the earth.</description>
        <graphicData>
            <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <color>(204, 195, 173)</color>
            <colorTwo>(110, 103, 93)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1500</MaxHitPoints>
        </statBases>
        <deepCommonality>0.3</deepCommonality>
        <deepCountPerPortion>40</deepCountPerPortion>
        <deepLumpSizeRange>4~12</deepLumpSizeRange>
