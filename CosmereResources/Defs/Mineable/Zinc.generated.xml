<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="RockBase">
        <defName>MineableZinc</defName>
        <label>Zinc Ore</label>
        <description>Unrefined Zinc ore mined directly from the earth.</description>
        <graphicData>
            <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <color>(128, 142, 129)</color>
            <colorTwo>(187, 189, 187)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1360</MaxHitPoints>
        </statBases>
        <deepCommonality>0.6</deepCommonality>
        <deepCountPerPortion>40</deepCountPerPortion>
        <deepLumpSizeRange>30~55</deepLumpSizeRange>
        <building>
            <isResourceRock>true</isResourceRock>
            <veinMineable>true</veinMineable>
            <mineableThing>Zinc</mineableThing>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.6</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>30~55</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereResources.DefModExtension.MetalsLinked">
                <metal>Zinc</metal>
            </li>
        </modExtensions>
    </ThingDef>
</Defs>