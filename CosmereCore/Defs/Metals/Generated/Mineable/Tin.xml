        <defName>MineableTin</defName>
        <label>Tin Ore</label>
        <description>Unrefined Tin ore mined directly from the earth.</description>
        <graphicData>
            <color>(197, 202, 201)</color>
            <colorTwo>(128, 137, 137)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1400</MaxHitPoints>
        </statBases>
        <building>
            <mineableThing>Tin</mineableThing>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.8</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>35~65</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereScadrial.DefModExtensions.MetalsLinked">
                <metals>
                    <li>Tin</li>
                </metals>
            </li>
        </modExtensions>
    </ThingDef>
