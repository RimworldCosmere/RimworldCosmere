<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Electrum -->
    <GeneDef ParentName="AbstractAllomancyInvestitureGene">
        <defName>Cosmere_Misting_Electrum</defName>
        
        <label>electrum misting</label>
        <labelShortAdj>oracle</labelShortAdj>
        
        <description>Shows one&#x27;s own possible futures, useful for avoiding danger.</description>
        
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>17</displayOrderInCategory>
        
        <resourceLabel>electrum</resourceLabel>
        <resourceDescription>Electrum is an alloy of gold and silver, characterized by its soft, pale, and luminous hue. This unique blend of precious metals has captivated civilizations for millennia with its subtle brilliance and inherent value.\n\nShows one&#x27;s own possible futures, useful for avoiding danger.</resourceDescription>
        
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Electrum</def>
            </li>
        </forcedTraits>
        
        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Electrum</metal>
            </li>
        </modExtensions>
        
        <abilities>
            <li>Cosmere_Ability_Electrum</li>
        </abilities>
        
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Electrum</iconPath>
    </GeneDef>
    
    <TraitDef ParentName="AbstractMetallicArtsInvestitureTrait">
        <defName>Cosmere_Misting_Electrum</defName>
        <label>oracle</label>
        <description>Shows one&#x27;s own possible futures, useful for avoiding danger.</description>
        
        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Electrum</metal>
            </li>
        </modExtensions>
        
        <degreeDatas>
            <li>
                <label>misting: oracle</label>
                <description>Shows one&#x27;s own possible futures, useful for avoiding danger.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
    <!-- Ferring: Electrum -->
    <GeneDef ParentName="AbstractFeruchemyInvestitureGene">
        <defName>Cosmere_Ferring_Electrum</defName>
        
        <label>electrum ferring</label>
        <labelShortAdj>pinnacle</labelShortAdj>
        
        <description>Stores determination or willpower.</description>
        
        <displayCategory>Cosmere_Scadrial_Feruchemy</displayCategory>
        <displayOrderInCategory>17</displayOrderInCategory>
        
        <forcedTraits>
            <li>
                <def>Cosmere_Ferring_Electrum</def>
            </li>
        </forcedTraits>
        
        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Electrum</metal>
            </li>
        </modExtensions>
        
        <iconPath>UI/Icons/Genes/Investiture/Feruchemy/Electrum</iconPath>
    </GeneDef>
    <TraitDef ParentName="AbstractMetallicArtsInvestitureTrait">
        <defName>Cosmere_Ferring_Electrum</defName>
        <label>pinnacle</label>
        <description>Stores determination or willpower.</description>
        
        <modExtensions>
            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Electrum</metal>
            </li>
        </modExtensions>
        
        <degreeDatas>
            <li>
                <label>ferring: pinnacle</label>
                <description>Stores determination or willpower.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
</Defs>