<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Misting: Aluminum -->
    <GeneDef ParentName="AbstractInvestitureGene">
        <defName>Cosmere_Misting_Aluminum</defName>
        <label>Aluminum Misting</label>
        <labelShortAdj>Gnat (aluminum)</labelShortAdj>
        <description>Wipes away all other metal reserves.</description>
        <displayCategory>Cosmere_Scadrial_Allomancy</displayCategory>
        <displayOrderInCategory>10</displayOrderInCategory>
        <forcedTraits>
            <li>
                <def>Cosmere_Misting_Aluminum</def>
            </li>
        </forcedTraits>
        <modExtensions>
            <li Class="CosmereScadrial.DefModExtensions.MetalsLinked">
                <metals>
                    <li>aluminum</li>
                </metals>
            </li>
        </modExtensions>
        <iconPath>UI/Icons/Genes/Investiture/Allomancy/Aluminum</iconPath>
    </GeneDef>
    <TraitDef ParentName="AbstractInvestitureTrait">
        <defName>Cosmere_Misting_Aluminum</defName>
        <label>Gnat (aluminum)</label>
        <description>Wipes away all other metal reserves.</description>
        <modExtensions>
            <li Class="CosmereScadrial.DefModExtensions.MetalsLinked">
                <metals>
                    <li>aluminum</li>
                </metals>
            </li>
        </modExtensions>
        <degreeDatas>
            <li>
                <label>Misting: Gnat (aluminum)</label>
                <description>Wipes away all other metal reserves.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
