    <!-- Raw Tin Definition -->
    <ThingDef ParentName="RockBase">
        <defName>MineableTin</defName>
        <label>tin ore</label>
        <description>Unrefined tin ore mined directly from the earth.</description>
        <graphicData>
            <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
            <color>(245, 224, 183)</color>
            <colorTwo>(192, 192, 192)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>200</MaxHitPoints>
        </statBases>
        <building>
            <isResourceRock>true</isResourceRock>
            <mineableThing>Tin</mineableThing>
            <veinMineable>true</veinMineable>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.8</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>35~65</mineableScatterLumpSizeRange>
        </building>
    </ThingDef>

