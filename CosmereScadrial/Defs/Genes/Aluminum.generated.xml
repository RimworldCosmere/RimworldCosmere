            <li Class="CosmereResources.ModExtensions.MetalsLinked">
                <metal>Aluminum</metal>
            </li>
        </modExtensions>
        
        <degreeDatas>
            <li>
                <label>misting: gnat (aluminum)</label>
                <description>Shields {PAWN} from investiture. Also wipes away all other investiture reserves.</description>
                <degree>0</degree>
                <commonality>0</commonality>
            </li>
        </degreeDatas>
    </TraitDef>
    
