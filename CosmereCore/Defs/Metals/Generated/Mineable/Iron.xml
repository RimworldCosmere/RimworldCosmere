<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="AbstractMineable">
        <defName>MineableIron</defName>
        <label>Iron Ore</label>
        <description>Unrefined Iron ore mined directly from the earth.</description>
        <graphicData>
            <color>(121, 113, 111)</color>
            <colorTwo>(94, 77, 76)</colorTwo>
        </graphicData>
        <statBases>
            <MaxHitPoints>1500</MaxHitPoints>
        </statBases>
        <building>
            <mineableThing>Iron</mineableThing>
            <mineableYield>40</mineableYield>
            <mineableScatterCommonality>0.8</mineableScatterCommonality>
            <mineableScatterLumpSizeRange>35~65</mineableScatterLumpSizeRange>
        </building>
        <modExtensions>
            <li Class="CosmereCore.ModExtensions.MetalsLinked">
                <metal>Iron</metal>
            </li>
        </modExtensions>
    </ThingDef>
</Defs>